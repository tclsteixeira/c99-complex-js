{
  "name": "c99-complex",
  "version": "1.0.0",
  "type": "module",

  "main": "./dist/c99-complex.esm.js",
  "exports": {
    ".": {
      "import": "./dist/c99-complex.esm.js"
    }
  },
  "types": "./dist/c99-complex.d.ts",
  "scripts": {
    "build": "rollup -c && tsc && rollup -c rollup.config.dts.js && npm run clean",
    "clean": "rm -rf dist/types",
    "prepublishOnly": "npm run build",
    "start": "http-server ./src/examples",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "repository": {
    "url": "https://github.com/tclsteixeira/c99-complex-js.git"
  },
  "keywords": [
    "complex",
    "numbers",
    "math",
    "arithmetic",
    "c99"
  ],
  "files": [
    "dist/",
    "src/examples/",
    "LICENSE",
    "LICENSE.BOOST-1.0",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "README.md"
  ],
  "author": "Tiago C. Teixeira",
  "license": "MIT and BSL-1.0",
  "description": "A fast, accurate standalone JavaScript library for complex number arithmetic compliant with C99 standards",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.5.2",
    "vitest": "^3.1.4",
    "http-server": "^14.1.1"
  }
}
